{
  "manifest_version": 2,

  "name": "Clickjack Pro",
  "author" : "Jijojohn",
  "description": "Clickjack Pro is a useful tool developed by jijojohn for checking Clickjacking vulnerabilities .",
  "version": "1.1",

  "permissions": [ "tabs"],
  
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
 
 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "cbid.html"
  },

"background": {
    "scripts": ["jquery.js","external.js"]
    
  }

}